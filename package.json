{
  "name": "react-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://kongwm.github.io/react-19/",
  "scripts": {
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
    "format": "biome format --write",
    "preview": "rsbuild preview",
    "test": "rstest",
    "test:watch": "rstest --watch",
    "prepare": "husky",
    "deploy": "gh-pages -d dist",
    "pages": "npm run build && cp dist/index.html dist/404.html && npm run deploy"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@rsbuild/core": "^1.6.14",
    "@rsbuild/plugin-babel": "^1.0.6",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rstest/core": "^0.7.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "cssnano": "^7.1.2",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --write"
    ]
  }
}
